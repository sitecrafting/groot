{
    "name": "sitecrafting/groot",
    "description": "The official SiteCrafting WordPress starter theme",
    "version": "1.1.3",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Coby Tamayo",
            "email": "ctamayo@sitecrafting.com"
        },
        {
            "name": "Reena Hensley",
            "email": "rhensley@sitecrafting.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "acobster/wp-cli-yaml-fixtures": "^0.6.0",
        "johnpbloch/wordpress-core-installer": "^1.0",
        "johnpbloch/wordpress-core": "^4.9",
        "mnsami/composer-custom-directory-installer": "^1.1",
        "sitecrafting/conifer": "^0.5",
        "squizlabs/php_codesniffer": "3.*",
        "wpackagist-plugin/timber-library": "^1.7",
        "wp-coding-standards/wpcs": "^0.14"
    },
    "extra": {
        "wordpress-install-dir": {
            "johnpbloch/wordpress-core": "wp"
        },
        "installer-paths": {
            "wp/wp-content/plugins/{$name}": ["sitecrafting/conifer"]
        }
    },
    "autoload": {
        "psr-4": {
            "Project\\": "lib/Project/"
        }
    }
}
