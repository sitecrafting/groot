{
    "name": "sitecrafting/groot",
    "description": "The official SiteCrafting WordPress starter theme",
    "version": "1.1.3",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Coby Tamayo",
            "email": "ctamayo@sitecrafting.com"
        },
        {
            "name": "Reena Hensley",
            "email": "rhensley@sitecrafting.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=8.1",
        "timber/timber": "^2.2"
    },
    "require-dev": {
        "acobster/wp-cli-yaml-fixtures": "^0.6.0",
        "johnpbloch/wordpress-core-installer": "^2.0",
        "johnpbloch/wordpress-core": "^6.5.1",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "sitecrafting/conifer": "dev-timber-2.x",
        "squizlabs/php_codesniffer": "3.*",
        "wp-coding-standards/wpcs": "^0.14"
    },
    "extra": {
        "wordpress-install-dir": {
            "johnpbloch/wordpress-core": "wp"
        }
    },
    "autoload": {
        "psr-4": {
            "Project\\": "lib/Project/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "johnpbloch/wordpress-core-installer": true,
            "mnsami/composer-custom-directory-installer": true
        }
    }
}
